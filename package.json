{
    "name": "tai_ger_portal_service",
    "version": "0.1.0",
    "bin": {
        "tai_ger_portal_service": "bin/tai_ger_portal_service.js"
    },
    "scripts": {
        "clean": "rm -rf build dist cdk.out node_modules",
        "build": "tsc",
        "lint": "eslint lib test constants --ext .ts",
        "lint:fix": "eslint lib test constants --ext .ts --fix",
        "format": "prettier --write .",
        "prebuild": "npm run format",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "prepare": "husky install",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "**/*.ts": [
            "eslint --fix",
            "prettier --write ."
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "22.7.9",
        "@typescript-eslint/eslint-plugin": "^8.21.0",
        "@typescript-eslint/parser": "^8.21.0",
        "aws-cdk": "2.173.4",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.4.2",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "~5.6.3"
    },
    "dependencies": {
        "aws-cdk-lib": "2.173.4",
        "constructs": "^10.0.0"
    }
}
